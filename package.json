  "devDependencies": {
    "@types/bun": "^1.3.8",
    "husky": "9.1.7",
    "lint-staged": "16.1.0",
    "prettier": "3.4.2",
    "prettier-plugin-svelte": "3.3.3",
    "prettier-plugin-tailwindcss": "0.6.11",
    "typescript": "^5.6.3"
  }
